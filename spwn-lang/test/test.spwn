constants = import "constants.spwn"

extract constants.obj_props
extract constants.comparisons
extract constants.easing_types


move = (group, x, y, duration: 0, easing: NONE, easing_rate: 2) {
	$.add(obj{
		OBJ_ID: 901,
		TARGET: group,
		MOVE_X: x * 3,
		MOVE_Y: y * 3,
		DURATION: duration,
		EASING: easing,
		EASING_RATE: easing_rate,
	})
	
}

move(10g, 100, 100, 0.5)



