newCell = (item, group) {
    let Cell = {item: counter(item), group: group};
    Cell.decrement = (self, by) {
        self.item-=by;
    }
    return Cell;
}

newBFSystem = (num) {
    let BFSystem = {cs: 0};
    newCell(4 as @item, 4 as @group);
    return BFSystem;
}

let system = newBFSystem(4);