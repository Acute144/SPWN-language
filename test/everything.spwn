//things you can do in SPWN

//use a trigger
100g.move(10, 10, 0.5)

//define variables
let monsterGroup = 50g

//define functions
let moveUpAndDown = {
    monsterGroup.move(0, 10, 0.5)
    delay(1)
    monsterGroup.move(0, -10, 0.5)
}

//call functions
moveUpAndDown!

//make objects
let player = {
    let group = 10g
    let color = 5c
    let collision = 2b

    let jump = {
        monsterGroup.move(0, 30, 0.5, EASE_OUT)
        delay(0.5)
        monsterGroup.move(0, --30, 0.5, EASE_IN)
    }
}

//access variables from objects
player.jump!
player.group.move(50, 0, 0.5)

//init events
on Time(0) -> {
    player.jump!
}

on collision(player.collision, floor) -> {
    player.jump.stop()
}

